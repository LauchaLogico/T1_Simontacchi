   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "import numpy as np\n",
    "\n",
    "# Configurar visualizaciones\n",
    "sns.set(style=\"whitegrid\")\n",
    "plt.rcParams['figure.figsize'] = (12, 6)\n",
    "\n",
    "# Cargar el dataset\n",
    "file_path = \"primer_trabajo/Unido/Dataset/movies_api.csv\"  \n", #seguro tengas que cambiar el path del archivo
    "try:\n",
    "    df = pd.read_csv(file_path)\n",
    "except FileNotFoundError:\n",
    "    raise FileNotFoundError(f\"El archivo {file_path} no se encontró.\")\n",
    "\n",
    "# 1. Inspección general del dataset\n",
    "print(\"\\n\\n1. Información general del dataset:\\n\")\n",
    "print(df.info())\n",
    "print(\"\\nPrimeras 5 filas del dataset:\\n\")\n",
    "print(df.head())\n",
    "print(\"\\nTamaño del dataset:\", df.shape)\n",
    "\n",
    "# 2. Análisis de valores nulos\n",
    "print(\"\\n\\n2. Valores nulos por columna:\\n\")\n",
    "print(df.isnull().sum())\n",
    "\n",
    "# Visualización de valores nulos con heatmap\n",
    "plt.title(\"Mapa de calor de valores nulos\")\n",
    "sns.heatmap(df.isnull(), cbar=False, cmap='viridis')\n",
    "plt.show()\n",
    "\n",
    "# 3. Estadísticas descriptivas\n",
    "print(\"\\n\\n3. Estadísticas descriptivas de variables numéricas:\\n\")\n",
    "print(df.describe())\n",
    "\n",
    "# 4. Análisis de columnas categóricas\n",
    "categorical_cols = df.select_dtypes(include=['object']).columns\n",
    "print(\"\\n\\n4. Columnas categóricas:\\n\", categorical_cols.tolist())\n",
    "\n",
    "for col in categorical_cols:\n",
    "    print(f\"\\nValores únicos en la columna '{col}':\")\n",
    "    print(df[col].value_counts(dropna=False).head(10))\n",
    "\n",
    "# 5. Visualización de la distribución de variables numéricas\n",
    "numerical_cols = df.select_dtypes(include=['int64', 'float64']).columns\n",
    "\n",
    "print(\"\\n\\n5. Visualización de la distribución de variables numéricas\")\n",
    "for col in numerical_cols:\n",
    "    plt.figure()\n",
    "    sns.histplot(df[col], kde=True, bins=30)\n",
    "    plt.title(f\"Distribución de {col}\")\n",
    "    plt.xlabel(col)\n",
    "    plt.show()\n",
    "\n",
    "# 6. Matriz de correlación entre variables numéricas\n",
    "print(\"\\n\\n6. Matriz de correlación entre variables numéricas:\\n\")\n",
    "correlation_matrix = df[numerical_cols].corr()\n",
    "print(correlation_matrix)\n",
    "\n",
    "plt.figure(figsize=(10, 8))\n",
    "sns.heatmap(correlation_matrix, annot=True, cmap=\"coolwarm\", fmt=\".2f\")\n",
    "plt.title(\"Matriz de correlación\")\n",
    "plt.show()\n",
    "\n",
    "# 7. Gráficos de pares para variables numéricas seleccionadas\n",
    "if len(numerical_cols) > 1:\n",
    "    print(\"\\n\\n7. Visualización de relaciones entre variables numéricas\")\n",
    "    selected_cols = numerical_cols[:5]  # Tomar hasta 5 columnas numéricas\n",
    "    sns.pairplot(df[selected_cols].dropna())\n",
    "    plt.show()\n",
    "\n",
    "# 8. Resumen final de datos faltantes y tipos de datos\n",
    "print(\"\\n\\n8. Resumen de tipos de datos y nulos:\\n\")\n",
    "data_summary = pd.DataFrame({\n",
    "    'Tipos de Datos': df.dtypes,\n",
    "    'Nulos Totales': df.isnull().sum(),\n",
    "    'Porcentaje de Nulos': (df.isnull().sum() / df.shape[0]) * 100\n",
    "})\n",
    "print(data_summary.sort_values(by='Nulos Totales', ascending=False))\n",
    "\n",
    "# Guardar un informe resumen como CSV\n",
    "output_file = \"primer_trabajo/Unido/Informe/eda_resumen.csv\"\n",
    "data_summary.to_csv(output_file, index=True)\n",
    "print(f\"\\nResumen guardado en: {output_file}\")\n",
    "\n",
    "print(\"\\nEDA finalizado con éxito.\")\n"
   ]
  }
